<Window x:Class="Kidrasov_Trade.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Kidrasov_Trade"
        mc:Ignorable="d"
        Title="Kidrasov" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="res/icon.ico">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>
        <Image Source="res/logo.png " HorizontalAlignment="Left"></Image>
        <TextBlock Text="Каблук" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" Width="175" Height="30" HorizontalAlignment="RIght" Margin="5" Click="BtnBack_Click"></Button>
    </Grid>
    
</Window>
